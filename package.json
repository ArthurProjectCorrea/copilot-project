{
  "name": "copilot-project",
  "version": "1.1.1",
  "description": "Project configured for pnpm and Jest documentation integration.",
  "main": "index.js",
  "bin": {
    "copilot-config": "scripts/copilot-config.js"
  },
  "scripts": {
    "postinstall": "node scripts/postinstall.js",
    "postupdate": "node scripts/init-github-config.js",
    "prepack": "node scripts/init-github-config.js",
    "init-config": "node scripts/init-github-config.js",
    "lint": "eslint . --ext .js",
    "check-types": "tsc --noEmit",
    "build": "echo 'No build step required for plain JS. Add your build command here if needed.'",
    "format": "prettier --write .",
    "commit": "pnpm exec cz",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "release": "pnpm run build && pnpm run changeset:publish",
    "update:jest-docs": "node scripts/update-jest-docs-manual.js",
    "docs:jest": "node scripts/update-jest-docs-manual.js",
    "update:nest-docs": "node scripts/update-nest-docs-manual.js",
    "docs:nest": "node scripts/update-nest-docs-manual.js",
    "update:prisma-docs": "node scripts/update-prisma-docs-manual.js",
    "docs:prisma": "node scripts/update-prisma-docs-manual.js",
    "update:nextjs-docs": "node scripts/update-nextjs-docs-manual.js",
    "docs:nextjs": "node scripts/update-nextjs-docs-manual.js"
  },
  "keywords": [
    "github",
    "copilot",
    "ai",
    "configuration",
    "chatmodes",
    "prompts",
    "instructions",
    "development",
    "automation",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "files": [
    ".github",
    "docs",
    "scripts",
    "index.js"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md,html,vue}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.6",
    "@types/node": "^24.3.1",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.34.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "remark-mdx": "^3.1.1",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "unified": "^11.0.5"
  }
}
